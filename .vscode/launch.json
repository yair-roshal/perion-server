{
	"version": "7.7.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "debug node server",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/server/server.js",
  
			"console": "integratedTerminal",
			"outputCapture": "std",
  
			"env": {
				"NODE_ENV": "dev"
			}
		}
	]
  }